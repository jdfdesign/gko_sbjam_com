<%# cache([site, I18n.locale, 'navbar']) do %>
<!-- Header
================================================== -->
<nav class="navbar navbar-default">
  <%= render "locales_select" %>
  <div class="container">
    <div class="navbar-header">
    
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <%= link_to(image_tag("logo.png", :class => "img-responsive"), root_url, :alt => site.title, :class => "navbar-brand") %>
    </div>
    <div class="collapse navbar-collapse">
      <div class="shared">
        <%= link_to("https://www.facebook.com/pages/SB-JAM-MUSIK-FESTIVAL/114192415290425", :target => "_blank") do %>
          <%= image_tag("FB-f-Logo__blue_58.png", :alt => "Facebook logo")%>
        <% end %>
      </div>
      <%= render 'menu', :sections => site.primary_menu, :menu_name => 'navbar-nav' %> 
      
    </div> 
    
  </div>
  
</nav>
<%# end %>